{"id": "sucrose-globe","type": "globe","title": "Globe Chart","optionPresets": {"file": "globe_data"},"optionDefaults": {"auto_spin": "false"},"ui": {"[name=color_data]": {"bind": "function (d,v,$o) {\n        return this.bindControl(d, v, $o, function () {\n          var color = self.getData(self.data, 'color_data'),\n              options = self.getColorOptions();\n          self.Chart.colorData(color, options);\n          self.loadChart();\n        })\n      }","check": "new RegExp(/default|class|graduated|data/i)","events": "click.my","title": "Color Model","type": "radio","watch": "","values": [{"value": "default","label": "Default"},{"value": "class","label": "Class"},{"value": "graduated","label": "Graduated"},{"value": "data","label": "Data driven"}]},"[name=gradient]": {"bind": "function () { return; }","setChartOption": "function () { return; }","hidden": true},"[name=auto_spin]": {"setChartOption": "function (v,self) {\n        var value = sucrose.utility.toBoolean(v);\n        self.Chart.autoSpin(value);\n      }","check": "new RegExp(/true/i)","events": "change.my","title": "Auto Spin","type": "checkbox","values": [{"value": "true","label": "Enable"}]},"[name=direction]": {"hidden": true}}}